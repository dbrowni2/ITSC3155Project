<h1>Submit Data</h1>
<%= form_for :submit do |f| %>
  <p>
    <%= f.checkbox :Preference %><br>
    <%= f.checkbox :Location %><br>
    <%= f.checkbox :Purchases %><br>
  </p>
 
  <p>
    <%= f.label :Description %><br>
    <%= f.text_area :Description %>
  </p>
 
  <p>
    <%= f.submit %>
  </p>
<% end %>
