<%= form_for :submission, url: submissions_path do |f| %>
<p>
    <%= f.check_box :Preference %>
     <%= f.label :Preference %><br>
    <%= f.check_box :Location %>
     <%= f.label :Location%><br>
    <%= f.check_box :Purchases %><br>
     <%= f.label :Purchases %>
  </p>
 
  <p>
    <%= f.label :Description %><br>
    <%= f.text_area :Description %>
  </p>
 
  <p>
    <%= f.submit %>
  </p>
<% end %>